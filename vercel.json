
{
  "rewrites": [
    {
      "source": "/product/(.*)",
      "destination": "/api/og?id=$1"
    },
    {
      "source": "/p/(.*)",
      "destination": "/api/og?id=$1"
    },
    {
      "source": "/home",
      "destination": "/api/og?id=home"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
