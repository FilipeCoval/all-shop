
{
  "builds": [
    {
      "src": "api/og.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/p/:id",
      "destination": "/api/og?id=:id"
    },
    {
      "source": "/product/:id",
      "destination": "/api/og?id=:id"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}


